{
  "name": "dotnet-harness-toolkit",
  "version": "0.0.1",
  "description": "Comprehensive .NET development toolkit for Gemini CLI",
  "contextFileName": "GEMINI.md",
  "mcpServers": {
    "serena": {
      "description": "Semantic code analysis and editing toolkit",
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant",
        "--enable-web-dashboard",
        "false",
        "--project",
        "."
      ],
      "env": {}
    },
    "context7": {
      "description": "Library documentation search server",
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ],
      "env": {}
    },
    "microsoftdocs-mcp": {
      "description": "Microsoft Learn documentation MCP",
      "type": "http",
      "url": "https://learn.microsoft.com/api/mcp"
    },
    "mcp-windbg": {
      "description": "WinDbg crash and dump analysis MCP",
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/svnscha/mcp-windbg",
        "mcp-windbg"
      ],
      "env": {}
    }
  }
}
