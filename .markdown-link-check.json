{
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "5s",
  "aliveStatusCodes": [200, 206],
  "ignorePatterns": [
    {
      "pattern": "^https://github.com/rudironsoni/.*"
    },
    {
      "pattern": "^https://localhost"
    },
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^file://"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^.",
      "replacement": "{{BASEURL}}/"
    }
  ]
}
